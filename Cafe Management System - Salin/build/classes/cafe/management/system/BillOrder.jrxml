	<queryString language="SQL">
		<![CDATA[SELECT 
    p.id_penjualan AS bill_id,
    k.nama_kasir,
    w.nama_waiters,
    c.nama_customers,
    m.poin_member,
    p.tanggal_penjualan AS tanggal_transaksi,
    pr.nama_produk,
    p.total_harga,
    jp.nama_jenis AS jenis_pembayaran,
    pb.status_bayar

FROM penjualan p
LEFT JOIN pembayaran pb ON pb.id_penjualan = p.id_penjualan
LEFT JOIN kasir k ON k.id_kasir = pb.id_kasir
LEFT JOIN waiters w ON w.id_waiters = p.id_waiters
LEFT JOIN customers c ON c.id_customers = p.id_customers
LEFT JOIN membership m ON m.id_customers = c.id_customers
LEFT JOIN jenis_pembayaran jp ON jp.id_jenis = pb.id_jenis
LEFT JOIN dipesan_dalam dd ON dd.id_penjualan = p.id_penjualan
LEFT JOIN produk pr ON pr.id_produk = dd.id_produk;
]]>
	</queryString>
